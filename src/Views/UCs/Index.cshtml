@model IEnumerable<BES.Models.Data.UC>

@{
    ViewData["Title"] = "Index";
}
<h2>Index</h2>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UCName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UCCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tehsil)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UCName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UCCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tehsil.TehsilName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UCID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UCID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UCID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
